    "#What is the number of add numbers in the range 0-700?\n",
    "even_count = 0\n",
    "odd_count = 0\n",
    "numbers = list(range(0,701))\n",
    "\n",
    "for num in numbers:\n",
    "    if num % 2 == 0:\n",
    "        even_count += 1\n",
    "    else:\n",
    "        odd_count += 1\n",
    "\n",
    "print('Even numbers: ', even_count)\n",
    "print('Odd numbers: ', odd_count)"
   ]
